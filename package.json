{
  "name": "ballistic",
  "version": "1.0.0",
  "description": "Ballistic 3D WebGL Web Game",
  "main": "index.html",
  "scripts": {
    "test": "none",
    "dev": "webpack-dev-server --host localhost --port 8080 --env=dev --inline --hot --profile --open",
    "debugHot": "webpack-dev-server --host localhost --port 8080 --env=dev --inline --hot --profile",
    "debug": "webpack-dev-server --host localhost --port 8080 --env=dev --profile",
    "net": "webpack-dev-server --public --host 192.168.0.178 --port 80 --env=dev --profile --open",
    "net2": "webpack-dev-server --public --host 192.168.0.170 --port 80 --env=dev --profile --open",
    "devHTML": "webpack-dev-server --progress --colors --host localhost --port 8080 --env=dev --profile --open",
    "watch": "webpack --progress --colors --env=dev --profile --watch",
    "exportp": "webpack --env=prod --profile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtreemze/Ballistic.git"
  },
  "author": "Carlos Velasco",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/xtreemze/Ballistic/issues"
  },
  "homepage": "https://github.com/xtreemze/Ballistic#readme",
  "devDependencies": {
    "@types/babylon": "^6.16.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.26.4",
    "extract-loader": "^0.1.0",
    "file-loader": "^0.10.1",
    "google-closure-compiler-js": "^20180101.0.0",
    "html-loader": "^0.4.5",
    "html-minifier-webpack-plugin": "^1.1.1",
    "imagemin-webpack-plugin": "^2.0.0",
    "offline-plugin": "^4.9.0",
    "prettier-eslint": "^8.8.1",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "extract-text-webpack-plugin": "^2.1.2"
  }
